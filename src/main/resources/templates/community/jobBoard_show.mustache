<!-- 직업게시판 상세 -->
{{>layouts/header}}
<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#jobBoard}}
        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </tr>
    {{/jobBoard}}
    </tbody>
</table>
<a href="/community/jobBoard/{{jobBoard.id}}/edit" class="btn btn-primary">수정</a>
<a href="/community/jobBoard/{{jobBoard.id}}/delete" class="btn btn-danger">삭제</a>
<a href="/community/jobBoard">목록</a>
{{>layouts/footer}}