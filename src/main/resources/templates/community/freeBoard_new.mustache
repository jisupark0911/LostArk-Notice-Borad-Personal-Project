<!-- 자유게시판 생성 -->
{{>layouts/header}}
<!doctype html>
<html>
<head>

    <title>Document</title>
    <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
    <link rel="stylesheet" type="text/css" href="/css/freeBoardNew.css">

</head>
<body>
<div class="freeBoardNew-container">
<form class="container" action="/community/freeBoard/create" method="post">
    <div class="mb-3">
        <label  class="form-label">제목</label>
        <input type="text" class="form-control" name="title" maxlength="255">
    </div>
    <div class="mb-3">
        <label class="form-label">내용</label>
        <textarea class="form-control" rows="3" name="content" maxlength="255"></textarea>
    </div>

    <button type="submit" class="write-button">작성</button>
    <a href="/community/freeBoard" style="text-decoration: none;">뒤로 가기</a>
</form>
</div>


</body>
</html>

{{>layouts/footer}}