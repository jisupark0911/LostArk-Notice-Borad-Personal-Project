<!-- 자유게시판 -->
{{>layouts/header}}

<link href="https://cdn.jsdelivr.net/npm/bootstrap@5.0.2/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-EVSTQN3/azprG1Anm3QDgpJLIm9Nao0Yz1ztcQTwFspd3yD65VohhpuuCOmLASjC" crossorigin="anonymous">
<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>
    {{#freeBoardList}}
        <tr>
            <th>{{id}}</th>
            <td><a href="/community/freeBoard/{{id}}">{{title}}</a></td>
            <td>{{content}}</td>
        </tr>
    {{/freeBoardList}}
    </tbody>
</table>

<button type="button" class="btn btn-primary"><a href="/community/freeBoard/new">글작성</a></button>
{{>layouts/footer}}